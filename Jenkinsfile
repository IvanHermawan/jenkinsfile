pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Build process'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy Process'
            }
        }
        stage('Test') {
            steps {
                echo 'Test Process'
            }
        }
        stage('Release') {
            steps {
                echo 'Test Release'
            }
        }
    }
}
